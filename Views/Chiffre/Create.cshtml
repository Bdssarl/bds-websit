@{
    Layout = "~/Views/Administration/_Layout.cshtml";
}
@model bds_site_web_version7_.Models.Chiffre
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Bds_site_web</title>
    <!--<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />-->
    <!--- <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />-->
    <link rel="stylesheet" href="~/css/Create.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Bds_site_web.styles.css" asp-append-version="true" />
</head>
<body>
    <div class="container">
        <div class="container-title">
            <h1>Enregistrer les chiffres </h1>
            
        </div>

        <form class="form" asp-controller="Chiffre" asp-action="Create" method="post" enctype="multipart/form-data">

            <div class="form_DomaineStage"> <label asp-for="AnneeExperience">Annee d'experience'</label><input class="input-button" type="text" required asp-for="AnneeExperience" /></div>
            <div class="form_DomaineStage"> <label asp-for="NombreProjetsRealises">nombre de Projets </label><input class="input-button" type="text" required asp-for="NombreProjetsRealises" /></div>
            <div class="form_DomaineStage"> <label asp-for="NombreEntreprisePartenaires">nombre de Partenaires </label><input class="input-button" type="text" required asp-for="NombreEntreprisePartenaires" /></div>
            <div class="form_submit"> <button class="submit-button">Enregistrer</button></div>
        </form>

    </div>
</body>
</html>