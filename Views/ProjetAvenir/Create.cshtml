@{
    Layout = "~/Views/Administration/_Layout.cshtml";
    ViewData["Title"] = "Projet A venir";
}
@model bds_site_web_version7_.Models.ProjetAVenir
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <!--<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />-->
    <!--- <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />-->
    <link rel="stylesheet" href="~/css/Create.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Bds_site_web.styles.css" asp-append-version="true" />
</head>
<body>
    <div class="container-projet-realise">
        <div class="container-title">
            <h1>Enregistrer un Projet A venir  </h1>
        </div>

        <form class="form-projet-realise" asp-controller="ProjetAvenir" asp-action="Create" method="post" enctype="multipart/form-data">
            <div class="form_DateDebutProjetRealise"><label asp-for="Id">Id</label><input type="text" required class="input-button" asp-for="Id" readonly /></div>
            <div class="form_DateDebutProjetRealise"><label asp-for="Idee">idee</label><input type="text" required class="input-button" asp-for="Idee" /><span asp-validation-for="Idee" class="text-danger"></span></div>
            <div class="form_DateDebutProjetRealise"><label asp-for="Nom_Page">nom de la page </label><input type="text" required class="input-button" asp-for="Nom_Page" /><span asp-validation-for="Nom_Page" class="text-danger"></span></div>
            <div class="cv-file"><input type="file" required class="input-button" asp-for="FormFile" /></div>

            <div class="form_submit"> <button class="submit-button" onclick="location.reload()">Enregistrer</button></div>
        </form>

    </div>
    

</body>
</html>
