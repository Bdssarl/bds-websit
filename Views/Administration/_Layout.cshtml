                <a class="nav-link"asp-controller="Partenaire" asp-action="Create"> Partenaires </a>
                 <a class="nav-link" asp-controller="Temoignages" asp-action="Create">Temoignages</a>
                <a class="nav-link" asp-controller="Images" asp-action="Create">Images</a>
                <a class="nav-link" asp-controller="Videos" asp-action="Create">Videos</a>
                <a class="nav-link"asp-controller="Stages" asp-action="Create">Stages</a>
                <a class="nav-link" asp-controller="TypeStage"asp-action="Create">Type de stage</a>
        <a class="nav-link" asp-controller="Chiffre" asp-action="Create">Nos chiffres</a>
        <a class="nav-link" asp-controller="Interêt" asp-action="Interêt"> interêts</a>
                </div>
       

        <div class="main"role="main">
        @RenderBody()
            
        </div>
      





        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        @await RenderSectionAsync("Scripts", required: false)
    </body>
</html>